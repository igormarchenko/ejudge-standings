<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <changeSet id="1" author="Igor Marchenko">
        <loadData tableName="university"
                  file="data/university.csv"
                  separator=";"/>
    </changeSet>

    <changeSet id="2" author="Igor Marchenko">
        <loadData tableName="teams"
                  file="data/teams.csv"
                  separator=";"/>
    </changeSet>

    <changeSet id="3" author="Igor Marchenko">
        <insert tableName="contest">
            <column name="id" value="1"/>
            <column name="is_final" value="true"/>
            <column name="name" value="Test Contest"/>
        </insert>
    </changeSet>
    <changeSet id="4" author="Igor Marchenko">
        <insert tableName="standings_files">
            <column name="id" value="1"/>
            <column name="link" value="http://ejudge.sumdu.edu.ua/external.xml"/>
            <column name="is_frozen" value="true"/>
            <column name="contest_id" value="1"/>
        </insert>
    </changeSet>


    <changeSet id="5" author="Igor Marchenko">
        <insert tableName="users">
            <column name="id" value="1"/>
            <column name="login" value="admin"/>
            <column name="pass" value="12345"/>
            <column name="role" value="ADMIN"/>
            <column name="enabled" value="TRUE"/>
        </insert>
    </changeSet>

    <changeSet id="6" author="Igor Marchenko">
        <loadData tableName="university"
                  file="data/university2017.csv"
                  separator=";"/>
    </changeSet>

    <changeSet id="7" author="Igor Marchenko">
        <loadData tableName="teams"
                  file="data/teams2017.csv"
                  separator=";"/>
    </changeSet>
</databaseChangeLog>
